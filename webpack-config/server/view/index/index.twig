{% extends 'layout.twig' %}
{% set lang = 'en' %}
{% set title = "furrytail" %}
{% set description = "Furlosophy is the new philosophy." %}
{% set keywords = "furrytail 白白猫窝" %}
{% set scripts = ['/dist/js/index/index.js'] %}

{% block main %}
  <div class="main cd-clearfix cd-container">
    <div class="cd-md-20">
      <div class="cd-panel">
        <div class="cd-panel-heading">
          <div class="cd-panel-title">
          <a href="{{ url('index') }}">面板标题</div>
          <div class="cd-panel-action">
            <a href="javascript:;" class="cd-link-major" data-toggle="cd-tooltip" data-title="这里可添加操作"><i class="cd-icon cd-icon-info-o"></i></a>
          </div>
        </div>
        <div class="cd-panel-body">
          面板内容
        </div>
      </div>
    </div>
    <div class="cd-md-4">
      <div class="cd-card">
        <div class="cd-card-image">
          <img class="cd-full" src="image/joker.jpg" alt=""/>
        </div>
        <div class="cd-card-body">敏捷估算扑克</div>
      </div>
    </div>
  </div>
{% endblock %} 